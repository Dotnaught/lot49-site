{
  "name": "lot49-site",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "npm run build && wrangler pages dev .svelte-kit/cloudflare",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "deploy": "npm run build && wrangler pages deploy .svelte-kit/cloudflare",
    "build-cf-types": "wrangler types && mv worker-configuration.d.ts src/"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250718.0",
    "@skeletonlabs/skeleton": "^3.1.7",
    "@skeletonlabs/skeleton-svelte": "^1.3.1",
    "@skeletonlabs/tw-plugin": "^0.4.1",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/adapter-cloudflare": "^7.1.0",
    "@sveltejs/kit": "^2.25.1",
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "@tailwindcss/postcss": "^4.1.11",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "svelte": "^5.36.8",
    "svelte-check": "^4.3.0",
    "tailwindcss": "^4.1.11",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^7.1.5",
    "wrangler": "^4.25.0"
  },
  "type": "module",
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "eslint": "^9.31.0"
  }
}
